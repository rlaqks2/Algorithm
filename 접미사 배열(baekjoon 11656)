#include<iostream>
#include<algorithm>
#include<string>
#include<vector>
using namespace std;
int main() {
	vector<string> strr;//list string 

	string str;
	cin >> str;
	int n = str.size();
	for (int i = 0; i < n; i++) {
		strr.push_back(str.substr(i));
	}

	sort(strr.begin(), strr.end());//sort

	for(int i=0;i<n;i++)
	cout << strr[i] << "\n";
	
	return 0;
}
